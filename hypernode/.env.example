FACTORY_BASE_URL=http://localhost:3101
HYPERNODE_NAME=fabrik-hypernode
# Optional seed for deterministic ID; leave empty to auto-derive
HYPERNODE_SEED=
# Optional fixed ID (overrides seed-based generation)
HYPERNODE_ID=
# Optional token (if Factory requires token). If empty, runner will handshake to fetch one.
HYPERNODE_TOKEN=
# JSON string of traits; keep compact and safe for .env files
HYPERNODE_TRAITS={"role":"community","cap":"observer"}
# Heartbeat interval seconds
HEARTBEAT_SECONDS=30
